    <SourceLine>txtLabel.Text = elem.Lookup("LabelName", "")</SourceLine>
    <SourceLine>txtColSpan.Text = elem.Lookup("ColSpan", "")</SourceLine>
    <SourceLine>popVarType.SetText(elem.Lookup("VarType", ""))</SourceLine>
    <SourceLine>txtVarName.Text = elem.Lookup("VarName", "")</SourceLine>
