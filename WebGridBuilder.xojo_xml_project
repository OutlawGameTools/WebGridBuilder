  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>2098532351</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function GetElementsForRow(rowID as String) As Dictionary()</SourceLine>
   <SourceLine>dim sql as String</SourceLine>
   <SourceLine>dim rs as RecordSet</SourceLine>
   <SourceLine>dim elems() as Dictionary</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>sql = "select * from Elements where RowID = " + rowID + " order by OrderBy"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>rs = srdata.SQLSelect(sql)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if rs = nil or rs.EOF then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>While Not rs.eof</SourceLine>
   <SourceLine>elems.Append(new Dictionary)</SourceLine>
   <SourceLine>dim idx as Integer = UBound(elems)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>elems(idx).Value("LabelName") = rs.Field("LabelName").StringValue</SourceLine>
   <SourceLine>elems(idx).Value("ElementID") = rs.Field("ElementID").StringValue</SourceLine>
   <SourceLine>rs.MoveNext</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Wend</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return elems</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
