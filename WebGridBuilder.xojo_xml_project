   <SourceLine>Sub AddTheseTables()</SourceLine>
   <SourceLine>// look for missing tables</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if not CheckForTable("libraries") then</SourceLine>
   <SourceLine>srdata.SQLExecute("CREATE TABLE libraries (libraryid integer NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE, name text, location text, includeas text default '')")</SourceLine>
   <SourceLine>end if</SourceLine>
