    <SourceLine></SourceLine>
    <SourceLine>btnDeleteColumn.Enabled = me.ListIndex &gt; -1</SourceLine>
    <SourceLine>grpColumnDetails.Enabled = me.ListIndex &gt; -1</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if me.ListIndex &gt; -1 then</SourceLine>
