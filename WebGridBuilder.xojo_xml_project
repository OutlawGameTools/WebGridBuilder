  <HookInstance>
   <ItemName>Action</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>1201364991</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub Action()</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>txtCode.Text = ""</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>LoadAllCodeParts</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>dim element,elements, row, rows, allCode as String</SourceLine>
    <SourceLine>dim rowSpan as Integer</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>dim dict() as Dictionary</SourceLine>
    <SourceLine>dim rowid as String</SourceLine>
    <SourceLine>for rowIdx as Integer = 0 to lstRows.ListCount-1</SourceLine>
    <SourceLine>// get all the rows</SourceLine>
    <SourceLine>rowid = lstRows.RowTag(rowIdx)</SourceLine>
    <SourceLine>dict = GetElementsForRow(rowid)</SourceLine>
    <SourceLine>rowSpan = 1</SourceLine>
    <SourceLine>//dim foo as Integer = dict.Ubound</SourceLine>
    <SourceLine>if dict.Ubound = -1 then</SourceLine>
    <SourceLine>// use as header </SourceLine>
    <SourceLine>row = CodeLegend.ReplaceAll("~legendname", lstRows.Cell(rowIdx, 0))</SourceLine>
    <SourceLine>else</SourceLine>
    <SourceLine>for elIdx as Integer = 0 to Ubound(dict)</SourceLine>
    <SourceLine>// get all elements for each row</SourceLine>
    <SourceLine>dim oneElem as Dictionary = GetElement(dict(elIdx).Lookup("ElementID", "0"))</SourceLine>
    <SourceLine>element = CodeField</SourceLine>
    <SourceLine>// check for SELECT</SourceLine>
    <SourceLine>if oneElem.LookUp("VarType", "") = "Select" then</SourceLine>
    <SourceLine>element = element.Replace("&lt;input type=" + chr(34) + "~vartype" + chr(34) + " id=" + chr(34) + "~id" + chr(34) + "&gt;", "&lt;select id=" + chr(34) + "~id" + chr(34) + "&gt;&lt;/select&gt;")</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>// check for TEXTAREA</SourceLine>
    <SourceLine>if oneElem.LookUp("VarType", "") = "TextArea" then</SourceLine>
    <SourceLine>element = element.Replace("&lt;input type=""~vartype"" id=""~id""&gt;", "&lt;textarea id=""~id""&gt;&lt;/textarea&gt;")</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>element = element.ReplaceAll("~label", oneElem.Lookup("LabelName", ""))</SourceLine>
    <SourceLine>element = element.ReplaceAll("~id", oneElem.Lookup("VarName", ""))</SourceLine>
    <SourceLine>element = element.ReplaceAll("~label", oneElem.Lookup("LabelName", ""))</SourceLine>
    <SourceLine>element = element.ReplaceAll("~vartype", oneElem.Lookup("VarType", ""))</SourceLine>
    <SourceLine>element = element.ReplaceAll("~colspan", oneElem.Lookup("ColSpan", ""))</SourceLine>
    <SourceLine>elements = elements + element</SourceLine>
    <SourceLine>rowSpan = rowSpan + Val(oneElem.Lookup("ColSpan", ""))</SourceLine>
    <SourceLine>next</SourceLine>
    <SourceLine>end</SourceLine>
    <SourceLine>if elements &lt;&gt; "" then</SourceLine>
    <SourceLine>row = CodeRow.ReplaceAll("~rowspan", Str(rowSpan-1))</SourceLine>
    <SourceLine>row = row.ReplaceAll("~fields", elements)</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if row &lt;&gt; "" then</SourceLine>
    <SourceLine>rows = rows + row</SourceLine>
    <SourceLine>end</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>elements = ""</SourceLine>
    <SourceLine>next</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>allCode = CodeForm.ReplaceAll("~rows", rows)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if chkIncludeHTML.Value then</SourceLine>
    <SourceLine>allCode = CodeHTML.Replace("~body", allCode)</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>txtCode.AppendText(allCode + App.eol)</SourceLine>
    <SourceLine>'</SourceLine>
    <SourceLine>'dim t as TidyDocumentMBS</SourceLine>
    <SourceLine>'</SourceLine>
    <SourceLine>'t=new TidyDocumentMBS</SourceLine>
    <SourceLine>'t.OptionXhtmlOut=True</SourceLine>
    <SourceLine>'</SourceLine>
    <SourceLine>'call t.ParseString(txtCode.Text)</SourceLine>
    <SourceLine>''call t.CleanAndRepair</SourceLine>
    <SourceLine>''call t.RunDiagnostics</SourceLine>
    <SourceLine>''t.OptionForceOutput=true</SourceLine>
    <SourceLine>'</SourceLine>
    <SourceLine>'txtCode.Text = t.SaveString</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
